<div class="w-full  ">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>



  <div id="tutorials" class="min-w-screen min-h-[90vh] bg-red-400 grid grid-flow-row grid-cols-4  justify-center">
    <%= render @tutorials%>
    <%= will_paginate @tutorials%>
  </div>
</div>
